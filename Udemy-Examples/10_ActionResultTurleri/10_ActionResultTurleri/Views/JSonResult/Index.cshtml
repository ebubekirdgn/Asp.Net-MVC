
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
    <script src="~/Scripts/jquery-3.3.1.min.js"></script>
</head>
<body>
    <div>
        <h2>JsonResult</h2>
    </div>
    <div>
        <input type="button" id="btnJsonResult" name="btn" value="Veri Getir" />
    </div>

    <script>
        $(function () {
            $("#btnJsonResult").click(function () {
                $.ajax({
                    type: "GET",
                    url: "/JSonResult/Index_JSon",
                    contentType: "application/json; charsert=utf-8",
                    dataType: "json",
                    success: function (e) {
                        alert(e.Id); alert(e.Ad); alert(e.Fiyat); alert(e.Aciklama);
                    }
                });
            });
        });
    </script>
</body>
</html>
